{
    "manifest_version": 2,
  
    "name": "New Tab",
    "description": "Replace default new tab page in Firefox with a useful new tab page.",
    "version": "2.0",
  
    "chrome_url_overrides" : {
      "newtab": "svelteapp/dist/index.html"
    },
    "permissions": [
      "bookmarks",
      "storage"
    ],

    "browser_specific_settings": {
      "gecko": {
        "id": "{05207257-41fa-4a72-bc2d-ec7b782534f7}",
        "strict_min_version": "42.0"
      }
    }
  }